{
  "scripts": {
    "build": "tsc --project tsconfig.json && bun run copy-sql",
    "copy-sql": "mkdir -p dist/src/migrations/sql && cp src/migrations/sql/*.sql dist/src/migrations/sql/",
    "start": "bun run index.ts",
    "lint": "npx prettier . --check; npx eslint .; tsc --project tsconfig.json --noEmit",
    "lint:ci": "npx prettier . --check && npx eslint . && tsc --project tsconfig.json --noEmit",
    "format": "npx prettier . --write",
    "test:unit": "bun test src/__tests__/unit",
    "test:integration": "bun test src/__tests__/integration",
    "test:e2e": "bun test src/__tests__/e2e",
    "buntest": "echo 'hello'"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.4",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "bun-types": "^1.1.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^11.0.0"
  },
  "name": "grouper",
  "module": "index.ts",
  "type": "module",
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
